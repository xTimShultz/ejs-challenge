<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form class="" action="/compose" method="post">
        <div class="form-group">
            <label>Title</label>
            <input type="text" class="form-control" name="postTitle" autofocus>
        </div>
        <div class="form-group">
            <label>Post</label>
            <textarea type="text" rows="3" class="form-control" name="postBody"></textarea>
        </div>
        <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>
<%- include("partials/footer"); -%>
